    "spark.sql.files.ignoreCorruptFiles": "true",
    "spark.memory.offHeap.enabled": "true",
    "spark.memory.offHeap.size": "50g",
    "spark.executor.cores": "5",
    "spark.executor.instances": "10"
  }
